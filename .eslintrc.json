{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "react/prop-types": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-unused-vars": "off",
    "no-plusplus": "off",
    "no-console": "off"
  }
}
